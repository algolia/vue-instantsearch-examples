{
  "name": "vue-examples",
  "repository": "https://github.com/algolia/vue-instantsearch-examples.git",
  "private": true,
  "scripts": {
    "lint": "eslint examples/**/*{.js,.vue} --ext .js,.vue",
    "build": "scripts/build.sh",
    "format": "prettier --write **/*.{js,vue,json}",
    "test": "yarn lint && yarn build"
  },
  "workspaces": ["examples/*"],
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-algolia": "13.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "prettier": "1.11.1"
  }
}
